{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Disco</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nse</th>
                <td>{{ entity.nse }}</td>
            </tr>
            <tr>
                <th>Fab</th>
                <td>{{ entity.fab }}</td>
            </tr>
            <tr>
                <th>Cap</th>
                <td>{{ entity.cap }}</td>
            </tr>
            <tr>
                <th>Modelo</th>
                <td>{{ entity.modelo }}</td>
            </tr>
            <tr>
                <th>Tip</th>
                <td>{{ entity.tip }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('disco') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('disco_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
